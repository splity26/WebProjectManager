@model IEnumerable<WebProjectManager.Data.Users>

@{
    ViewBag.Title = "Users available";
}
<div class="span10">
    <h2>Users</h2>

    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Enroll Date</th>
                <th scope="col">Active?</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Username</td>
                    <td>@user.EnrollDate.ToShortDateString()</td>
                    <td>@user.IsActive</td>
                </tr>
            }
        </tbody>
    </table>
</div>


