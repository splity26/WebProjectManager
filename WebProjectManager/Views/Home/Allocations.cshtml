@{
    ViewBag.Title = "Allocations";
}

<h2>Allocation</h2>

<div class="span10">
    <h2>Projects</h2>

    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Assignee</th>
                <th scope="col">Project</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var alloc in Model)
            {
                <tr>
                    <td>@alloc.Username</td>
                    <td>@alloc.ProjectName</td>
                </tr>
            }
        </tbody>
    </table>
</div>


